<?xml version='1.0' encoding='utf-8'?>
<widget version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <access origin="*" />
  
  <feature name="Device">
    <param name="android-package" value="org.apache.cordova.device.Device"/>
  </feature>

  <feature name="UniqueDeviceID">
    <param name="android-package" value="hu.dpal.phonegap.plugins.UniqueDeviceID"/>
  </feature>

  <feature name="NfcPlugin">
    <param name="android-package" value="com.chariotsolutions.nfc.plugin.NfcPlugin"/>
    <param name="onload" value="true"/>
  </feature>

  <feature name="Permissions">
    <param name="android-package" value="com.android.plugins.Permissions"/>
  </feature>

  <feature name="UID">
    <param name="android-package" value="org.hygieiasoft.cordova.uid.UID"/>
  </feature>

  <feature name="UniqueDeviceID">
    <param name="android-package" value="hu.dpal.phonegap.plugins.UniqueDeviceID"/>
  </feature>

  
</widget>